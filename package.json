{
  "name": "shopify-processor",
  "version": "1.0.0",
  "description": "A TypeScript application for processing Shopify products with BullMQ queue system",
  "main": "dist/app.js",
  "keywords": [
    "shopify",
    "queue",
    "typescript",
    "bullmq",
    "product-processing"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node dist/app.js",
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "graphql-codegen": "graphql-codegen",
    "lint": "eslint . --ext .ts",
    "test:sync-queue": "ts-node src/tests/test-sync-queue.ts"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@shopify/admin-api-client": "^1.0.8",
    "bullmq": "^4.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "mongoose": "^7.4.1",
    "uuid": "^11.1.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@shopify/api-codegen-preset": "^1.1.7",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.17.32",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint": "^8.46.0",
    "jest": "^29.6.2",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  }
}
